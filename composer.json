{
    "name": "luccpl/orkestra",
    "description": "Orchestration for PHP applications",
    "type": "project",
    "license": "MIT",
    "version": "v1.0.1",
    "keywords": [
        "orkestra"
    ],
    "scripts": {
        "pint": "vendor/bin/pint",
        "pest": "XDEBUG_MODE=coverage vendor/bin/pest",
        "phpstan": "./vendor/bin/phpstan analyse --memory-limit=2048M src/ --level 9",
        "pest:coverage": "XDEBUG_MODE=coverage ./vendor/bin/pest --coverage --coverage-html=coverage"
    },
    "autoload": {
        "psr-4": {
            "Orkestra\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Lucas Carvalho",
            "email": "lucas.carvalhopl@outlook.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "pestphp/pest": "^2.34",
        "mockery/mockery": "^1.6",
        "laravel/pint": "^1.15"
    },
    "require": {
        "php": "^8.2",
        "php-di/php-di": "^7.0",
        "symfony/console": "^7.0",
        "symfony/process": "^7.0",
        "league/route": "^5.1",
        "laminas/laminas-diactoros": "^3.3",
        "laminas/laminas-httphandlerrunner": "^2.9",
        "rakit/validation": "^1.4",
        "twig/twig": "^3.0",
        "twig/markdown-extra": "^3.7",
        "fakerphp/faker": "^1.23",
        "larapack/dd": "^1.1"
    }
}
